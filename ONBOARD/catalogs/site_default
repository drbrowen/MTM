SERVER=https://munkidev.eps.uillinois.edu
PACKAGEPATH=ONBOARD/pkgs

OUTPACKAGE=/usr/local/munki/munkiconfig.pkg

SERIALNUMBER=`system_profiler SPHardwareDataType 2&gt;/dev/null |awk '/Serial Number \(system\)/ {print $4}'|base64`

PKGFILE="munkiconfig.php?ident=$SERIALNUMBER"

FULLURL="$SERVER/$PACKAGEPATH/$PKGFILE"

/usr/bin/python &lt;&lt;EOF
import sys
sys.path.append('/usr/local/munki');

from munkilib import fetch
from munkilib import munkicommon
from munkilib import keychain

keychain_obj = keychain.MunkiKeychain()

try:
  fetch.getResourceIfChangedAtomically('$FULLURL',
                                 '$OUTPACKAGE');
except Exception as e:
  pass

EOF

#mv /usr/local/munki/munkiconfig.php /usr/local/munki/munkiconfig.pkg

if [ -f $OUTPACKAGE ]; then
#    echo YES
